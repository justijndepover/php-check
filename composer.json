{
    "name": "justijndepover/php-check",
    "description": "Helper package to fix coding styles and run unit testing",
    "keywords": [
        "php",
        "check",
        "php-cs-fixer",
        "phpunit",
        "justijndepover"
    ],
    "homepage": "https://github.com/justijndepover/php-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/console": "^5.2",
        "symfony/filesystem": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Justijndepover\\PHPCheck\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "php-check"
    ]
}
